/*Contains all our tree data structures. Implemented based on Angelic Phan's class Diagram submission.
//Our BST class.
Class node //Or BST
{
  public:
  node & go_left();
  node & go_right();
  connect_left();
  connect_right();
  protected:
  node left;
  node right;
};

class service_history//BST for our service history.
{
  public:
  int read_from_file;
  int write_to_file;
  int add_service_history;
  protected:
  service_s_root;
  String comments;
  String date_of_data_entry;
  String p_name;
  int p_num;
  int m_num;
};

class service_list//BST for our list of services.
{
  public:
  int read_from_file;
  int write_to_file();
  int add_service();
  protected:
  service s_root;
};

class provider_list
{
  int add_provider();
  int remove_provider();
  int read_from_file();
  int write_to_file();
  protected:
  provider p_root;
};

class member_list
{
  public:
  int read_from_file();
  int write_to_file();
  int add_member();
  int update_member();
  int remove_member();
  protected:
  members m_root;
};
